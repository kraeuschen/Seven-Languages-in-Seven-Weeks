shell: irb

rb(main):001:0> puts 'hello, world'
hello, world
=> nil
irb(main):002:0> language = 'Ruby'
=> "Ruby"
irb(main):003:0> puts "hello, #{language}"
hello, Ruby
=> nil
irb(main):004:0> puts "hello, #{language}"
hello, Ruby
=> nil
irb(main):005:0> language = 'my Ruby'
=> "my Ruby"
irb(main):006:0> puts "hello, #{language}"
hello, my Ruby
=> nil
